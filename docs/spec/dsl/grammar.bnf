<program>         ::= <statement>*

<statement>       ::= <assignment>
                    | <function_call>
                    | <identifier>
                    | <literal>
                    | <string_literal>
                    | <argument_list>
                    | <vector_literal>
                    | <tuple_literal>

<assignment>      ::= <identifier> "=" <expression>

<function_call>   ::= <identifier> "(" <argument_list>? ")"

<argument_list>   ::= <argument> ("," <argument>)*

<argument>        ::= <assignment>
                    | <expression>

<expression>      ::= <literal>
                    | <string_literal>
                    | <identifier>
                    | <function_call>
                    | <argument_list>
                    | <vector_literal>
                    | <tuple_literal>

<vector_literal>  ::= "[" <expression_list>? "]"

<tuple_literal>   ::= "{" <expression_list>? "}"

<expression_list> ::= <expression> ("," <expression>)*

<literal>         ::= <number>
<number>          ::= [0-9]+ ("." [0-9]+)?

<string_literal>  ::= "\"" .*? "\""

<identifier>      ::= [a-zA-Z_][a-zA-Z0-9_]*
